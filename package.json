{
  "name": "filecoin-api",
  "version": "0.0.0",
  "description": "API client for Filecoin",
  "main": "src/index.js",
  "browser": {
    "node-fetch": false
  },
  "scripts": {
    "build": "webpack",
    "test": "npm run test:unit",
    "test:unit": "ava --verbose 'test/unit/**/*.js'",
    "test:e2e": "node test/e2e/_warn.js && ava --verbose 'test/e2e/**/*.js'",
    "coverage": "nyc --reporter=lcov --reporter=text ava --verbose"
  },
  "author": "Alan Shaw",
  "license": "",
  "dependencies": {
    "cids": "^0.5.7",
    "explain-error": "^1.0.4",
    "iterable-ndjson": "^1.0.0",
    "multiaddr-to-uri": "^4.0.1",
    "node-fetch": "^2.3.0",
    "string_decoder": "^1.2.0"
  },
  "devDependencies": {
    "ava": "^1.2.1",
    "babel-eslint": "^10.0.1",
    "nyc": "^13.3.0",
    "standard": "^12.0.1",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3"
  },
  "ava": {
    "babel": false,
    "compileEnhancements": false,
    "failWithoutAssertions": false
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
